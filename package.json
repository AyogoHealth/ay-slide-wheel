    "dist": "npm run lint && rm -rf dist/* && tsc -p tsconfig.dist.json",
    "lint": "eslint --ext .ts,.js .",
    "preversion": "npm test",
    "postversion": "git push && git push --follow-tags",
    "start": "npm run demo",
